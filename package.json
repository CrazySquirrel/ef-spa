    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": [
        "npm run lint"
      ],
      "pre-push": [
        "npm run lint",
        "npm run build:production"
      ],
      "post-commit": [
        "git status"
