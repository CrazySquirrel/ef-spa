    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run test:jest",
      "pre-push": [
        "npm run lint",
        "npm run test:jest",
        "npm run test:hermione",
        "npm run build:production"
      ],
      "post-commit": [
        "git status"
      ],
      "post-checkout": [
        "npm ci"
      ],
      "post-merge": [
        "npm ci"
      ]
    }
  }
}
