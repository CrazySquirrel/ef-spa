  "scripts": {
    "build:development": "webpack --config ./config/webpack.development.config.js",
    "build:production": "webpack --config ./config/webpack.production.config.js",
    "build:development:watch": "webpack --config ./config/webpack.development.config.js --watch",
    "build:production:watch": "webpack --config ./config/webpack.production.config.js --watch",
    "build:app": "electron-builder -mwl --config ./electron-builder.js",
    "build:docs": "npm run build:gitbook && npm run build:storybook && npm run build:typedoc",
    "build:storybook": "build-storybook -c .storybook -o docs/storybook",
    "build:typedoc": "typedoc --options typedoc.json src/",
    "build:gitbook": "gitbook build ./.docs docs",
    "electron": "NODE_ENV=production electron ./electron/electron.js",
    "docker:build:app": "docker-compose -f docker-compose-electron.yml up --force-recreate",
    "docker:electron": "docker-compose -f docker-compose-electron.yml up --force-recreate",
    "docker:selenium": "docker-compose -f docker-compose-selenium.yml up --force-recreate",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "gitbook:serve": "gitbook serve ./.docs docs",
    "gitbook:build": "gitbook build ./.docs docs",
    "lint": "npm run lint:tslint && npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint -c .eslintrc.json '{.storybook,config,hermione,public,scripts,server,src,tests,utils}/**/*.{js,jsx}'",
    "lint:tslint": "tslint -c tslint.json --project tsconfig.json '{.storybook,config,hermione,public,scripts,server,src,tests,utils}/**/*.{ts,tsx}'",
    "lint:stylelint": "stylelint '{.storybook,config,hermione,public,scripts,server,src,tests,utils}/**/*.{css,scss}' --syntax scss",
    "test": "npm run test:jest && npm run test:hermione && npm run test:lighthouse",
    "test:jest": "jest",
    "test:hermione": "node ./hermione/index.js",
    "test:lighthouse": "mocha -r ts-node/register --recursive tests/lighthouse/index.ts",
    "postinstall": "npm run postinstall:git-lfs && npm run postinstall:husky && npm run postinstall:electron",
    "postinstall:git-lfs": "git lfs update --force",
    "postinstall:husky": "node node_modules/husky/lib/installer/bin install",
    "postinstall:electron": "electron-builder install-app-deps",
    "prepare": "npm run prepare:snyk",
    "prepare:snyk": "snyk protect"
  },
